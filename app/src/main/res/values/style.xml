<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Roboto_Medium" parent="TextAppearance.AppCompat">
        <item name="fontFamily">font/roboto_medium.ttf</item>
    </style>

    <style name="Lato_Regular" parent="TextAppearance.AppCompat">
        <item name="fontFamily">font/lato_regular.ttf</item>
    </style>

    <style name="Work_Sans" parent="TextAppearance.AppCompat">
        <item name="fontFamily">font/work_sans.ttf</item>
    </style>

    <style name="stylebold" parent="Roboto_Medium">
        <item name="android:textColor">@color/digiteam</item>
        <item name="android:textSize">@dimen/font16</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="deskripsi" parent="Lato_Regular">
        <item name="android:textColor">@color/deskripsi</item>
        <item name="android:textSize">@dimen/font12</item>
    </style>

    <style name="profile" parent="Lato_Regular">
        <item name="android:textColor">@color/digiteam</item>
        <item name="android:textSize">@dimen/font14</item>
    </style>

</resources>